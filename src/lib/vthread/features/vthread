hdr pthread
lib pthread_mutexattr_settype	-lpthread
lib pthread_mutexattr_setkind	-lpthread
lib pthread_mutexattr_setkind_np	-lpthread
num PTHREAD_MUTEX_RECURSIVE	pthread.h
num PTHREAD_MUTEX_RECURSIVE_NP	pthread.h
num MUTEX_RECURSIVE_NP	pthread.h
num PTHREAD_MUTEX_ERRORCHECK	pthread.h
num PTHREAD_MUTEX_ERRORCHECK_NP	pthread.h
num MUTEX_NONRECURSIVE_NP	pthread.h

lib pthread_mutexattr_setkind	-lcma
lib pthread_mutexattr_setkind_np	-lcma
lib pthread_mutexattr_create	-lcma
lib pthread_mutexattr_delete	-lcma
hpux pthread note{ Weird prototypes on HPUX }end compile{
	#include	<pthread.h>
	main()
	{	pthread_mutexattr_t	attr;
		pthread_mutex_t		mutex;
		pthread_mutex_init(&mutex, attr);
	}
}end
